<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="Forms.ModifyAppointment"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Pane prefHeight="400.0" prefWidth="600.0">
            <Label fx:id="tittleLbl" layoutX="184.0" layoutY="25.0" text="Modify Appointment">
                <font>
                    <Font name="Roboto Thin" size="30.0" />
                </font></Label>
            <Label layoutX="36.0" layoutY="106.0" text="Tittle">
                <font>
                    <Font size="15.0" />
                </font>
            </Label>
            <TextField fx:id="tittle" layoutX="86.0" layoutY="104.0" />
            <Label layoutX="358.0" layoutY="106.0" text="Type">
                <font>
                    <Font size="15.0" />
                </font>
            </Label>
            <ComboBox fx:id="typeOfApp" layoutX="410.0" layoutY="104.0" prefWidth="150.0" />
            <Label fx:id="userLbl1" layoutX="36.0" layoutY="151.0" text="Description">
                <font>
                    <Font size="15.0" />
                </font>
            </Label>
            <TextField fx:id="descriptionTxt" layoutX="36.0" layoutY="181.0" prefHeight="51.0" prefWidth="159.0" />
            <Label layoutX="226.0" layoutY="151.0" text="Location">
                <font>
                    <Font size="15.0" />
                </font>
            </Label>
            <TextField fx:id="locationTxt" layoutX="226.0" layoutY="181.0" />
            <Label fx:id="userLbl111" layoutX="36.0" layoutY="248.0" text="Contact Name">
                <font>
                    <Font size="15.0" />
                </font>
            </Label>
            <TextField fx:id="contactName" disable="true" layoutX="37.0" layoutY="276.0" />
            <Label fx:id="userLbl1111" layoutX="226.0" layoutY="248.0" text="Email">
                <font>
                    <Font size="15.0" />
                </font>
            </Label>
            <TextField fx:id="email" layoutX="226.0" layoutY="276.0" />
            <Label fx:id="userLbl21" layoutX="410.0" layoutY="151.0" text="Date">
                <font>
                    <Font size="15.0" />
                </font>
            </Label>
            <DatePicker fx:id="startDate" layoutX="410.0" layoutY="181.0" onAction="#getTime" />
            <Label fx:id="userLbl211" layoutX="417.0" layoutY="248.0" text="Time">
                <font>
                    <Font size="15.0" />
                </font>
            </Label>
            <ComboBox fx:id="time" layoutX="410.0" layoutY="276.0" prefWidth="150.0" />
            <Button layoutX="388.0" layoutY="331.0" mnemonicParsing="false" onAction="#save" text="Save" />
            <Button layoutX="449.0" layoutY="331.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
            <Label fx:id="lbl" layoutX="22.0" layoutY="335.0" />
        </Pane>
    </children>
</AnchorPane>
